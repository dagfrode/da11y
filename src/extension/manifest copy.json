{
  "name": "Da11y",
  "description": "Dags attempt at a A11y extention",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Click to open panel"
  },
  "side_panel": {
    "default_path": "main.html"
  },
  "permissions": ["sidePanel", "activeTab"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "Command+Shift+U"
      }
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
